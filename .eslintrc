{
  "extends": "eslint-config-airbnb-base",
  "parser": "babel-eslint",
  "plugins": [
    "babel"
  ],
  "rules": {
    "arrow-parens": 0,
    "babel/arrow-parens": 2,
    "generator-star-spacing": 0,
    "babel/generator-star-spacing": 2,
    "require-yield": 0
  },
  "env": {
    "node": true,
    "jasmine": true
  }
}